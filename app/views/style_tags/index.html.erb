<h1 class="inline">Style Tags</h1> <%= link_to "Add Tag", new_style_tag_path %>

<ul>
<% @style_tags.each do |tag| %>
  <li><%= tag.name %> <%= link_to "Edit", edit_style_tag_path(tag), class: "small" %> <%= link_to "Delete", style_tag_path(tag), method: :delete, class: "small" %></li>
<% end %>
</ul>
